{
  "name": "ng-packaged",
  "version": "1.1.20",
  "private": true,
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build:app": "cpr dist/ionic-components src/@moventes/componentes && ng build packages && rimraf src/@moventes",
    "build:lib": "ng-packagr -p componentes/package.json",
    "test": "ng test",
    "e2e": "ng e2e",
    "start": "ionic-app-scripts lint && ionic-app-scripts serve",
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "link": "yarn link mv-common-components && yarn install && npm run build:lib && cd dist/ionic-components && yarn link",
    "link:npm": "npm link mv-common-components && npm install && npm run build:lib && cd dist && npm link",
    "publish:yarn": "yarn install && npm run build:lib && echo \"Checking if you are logged in NPM registry (user: moventes, email: dev@moventes.com, pwd: *******)...\" && npm whoami && npm publish dist/ionic-components",
    "publish:npm": "npm install && npm run build:lib && echo \"Checking if you are logged in NPM registry (user: moventes, email: dev@moventes.com, pwd: *******)...\" && npm whoami && npm publish dist/ionic-components",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json",
    "compodoc-serve": "./node_modules/.bin/compodoc -s"
  },
  "dependencies": {
    "@angular/common": "6.0.0-rc.6",
    "@angular/compiler": "6.0.0-rc.6",
    "@angular/core": "6.0.0-rc.6",
    "@angular/forms": "6.0.0-rc.6",
    "@angular/http": "6.0.0-rc.6",
    "@angular/platform-browser": "6.0.0-rc.6",
    "@angular/platform-browser-dynamic": "6.0.0-rc.6",
    "@angular/router": "6.0.0-rc.6",
    "@ionic-native/core": "4.4.0",
    "@ionic-native/splash-screen": "4.4.0",
    "@ionic-native/status-bar": "4.4.0",
    "@ionic/storage": "2.1.3",
    "@ngx-translate/core": "^9.0.1",
    "@ngx-translate/http-loader": "^2.0.0",
    "@types/google-libphonenumber": "^7.4.15",
    "cordova-android": "6.3.0",
    "cordova-plugin-device": "^1.1.4",
    "cordova-plugin-ionic-webview": "^1.2.1",
    "cordova-plugin-splashscreen": "^4.0.3",
    "cordova-plugin-whitelist": "^1.3.1",
    "core-js": "^2.4.1",
    "google-libphonenumber": "^3.0.8",
    "ionic-angular": "^3.9.2",
    "ionic-plugin-keyboard": "^2.2.1",
    "ionicons": "3.0.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "mv-common-components": ">=1.1.3",
    "polyfill": "^0.1.0",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.2.0",
    "sw-toolbox": "^3.6.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.5.0",
    "@angular-devkit/schematics": "^0.5.0",
    "@angular/cli": "6.0.0-rc.7",
    "@angular/compiler-cli": "6.0.0-rc.6",
    "@angular/language-service": "6.0.0-rc.6",
    "@ionic/app-scripts": "3.1.2",
    "@types/jasmine": "~2.8.0",
    "@types/lodash": "^4.14.111",
    "@types/node": "~9.6.0",
    "codelyzer": "~4.3.0",
    "cpr": "^3.0.0",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.0",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^1.0.0",
    "ng-packagr": "3.0.0-rc.2",
    "protractor": "~5.3.0",
    "rimraf": "^2.6.1",
    "ts-node": "~6.0.0",
    "tsickle": "^0.27.3",
    "tslib": "^1.7.1",
    "tslint": "~5.9.0",
    "typescript": "~2.7.2"
  },
  "cordova": {
    "platforms": [
      "android"
    ],
    "plugins": {
      "ionic-plugin-keyboard": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {}
    }
  }
}
